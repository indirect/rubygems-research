<div id="<%= dom_id version %>">
  <p>
    <%= link_to version.rubygem.name, rubygem_path(version.rubygem.name) %>
  </p>

  <p>
    <strong>Number:</strong>
    <%= version.number %>
  </p>

  <p>
    <strong>Platform:</strong>
    <%= version.platform %>
  </p>

  <p>
    <strong>Spec sha256:</strong>
    <%= version.spec_sha256 %>
  </p>

  <p>
    <strong>Sha256:</strong>
    <%= version.sha256 %>
  </p>
  <p>
    <strong>Uploaded at:</strong>
    <%= time_tag version.uploaded_at %>
  </p>

</div>
